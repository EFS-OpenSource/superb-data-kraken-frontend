{
  "name": "SDK_Open_Source_Frontend",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "./src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "outputPath": "dist/SDK_Open_Source_Frontend"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        },
        "typecheck": {
          "executor": "nx:run-commands",
          "options": {
            "commands": ["tsc -p tsconfig.app.json --noEmit"],
            "cwd": "src/**/*.{ts, tsx}",
            "forwardAllArgs": false
          }
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "SDK_Open_Source_Frontend:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "SDK_Open_Source_Frontend:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "SDK_Open_Source_Frontend:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "SDK_Open_Source_Frontend:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "SDK_Open_Source_Frontend:build:development"
        },
        "production": {
          "buildTarget": "SDK_Open_Source_Frontend:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/SDK_Open_Source_Frontend"],
      "options": {
        "reportsDirectory": "coverage/SDK_Open_Source_Frontend",
        "jestConfig": "jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true,
        "coverage": true,
        "coverageReporters": ["lcov", "text-summary"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["./src/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "SDK_Open_Source_Frontend:build"
      }
    },

    "tags": {}
  }
}
